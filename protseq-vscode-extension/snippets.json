    "Report": {
        "prefix": "Report",
        "body": [
            "Report ->",
            "    HTML ->",
            "        Path: $1",
            "    <-",
            "<-"
        ],
        "description": "Default ReadsAlign structure"
    },
    "Peaks": {
        "prefix": "Peaks",
        "body": [
            "Peaks ->",
            "    Path: $1",
