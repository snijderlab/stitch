{
    "Structure": {
        "prefix": "Structure",
        "body": [
            "------| Assemble |------",
            "",
            "-Run Info---------------",
            "Version: 0",
            "Runname: $1",
            "RunType: ${2:Group}",
            "",
            "Assembly ->",
            "    Input ->",
            "        $3",
            "    <-",
            "    ",
            "    K: $4",
            "    Alphabet ->",
            "        Path: $5",
            "        Name: ${6:Assembly Alphabet}",
            "    <-",
            "<-",
            "",
            "Report ->",
            "    HTML ->",
            "        Path: $7",
            "    <-",
            "<-"
        ],
        "description": "Default Batch File structure"
    },
    "Assembly": {
        "prefix": "Assembly",
        "body": [
            "Assembly ->",
            "    Input ->",
            "        $1",
            "    <-",
            "    ",
            "    K: $2",
            "    Alphabet ->",
            "        Path: $3",
            "        Name: ${4:Assembly Alphabet}",
            "    <-",
            "<-"
        ],
        "description": "Default Assembly structure"
    },
    "Segment": {
        "prefix": "Segment",
        "body": [
            "Segment ->",
            "    Name: $1",
            "    Path: $2",
            "<-"
        ],
        "description": "Default Segment structure"
    },
    "SegmentFree": {
        "prefix": "SegmentFree",
        "body": [
