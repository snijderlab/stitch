            "",
            "    Segments ->",
            "        $4",
            "    <-",
            "",
            "    Alphabet ->",
            "        Path: $3",
            "        Name: ${4:Assembly Alphabet}",
            "        GapStartPenalty: ${5:12}",
            "        GapExtendPenalty: ${6:1}",
            "    <-",
            "<-"
        ],
        "description": "Default Recombine structure"
    },
    "ReadsAlign": {
        "prefix": "ReadsAlign",
        "body": [
            "ReadsAlign ->",
            "    Input ->",
            "        $1",
            "    <-",
            "",
            "    CutoffScore: $2",
            "",
            "    Alphabet ->",
            "        Path: $3",
            "        Name: ${4:Assembly Alphabet}",
            "        GapStartPenalty: ${5:12}",
            "        GapExtendPenalty: ${6:1}",
            "    <-",
            "<-"
        ],
        "description": "Default ReadsAlign structure"
